<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_map">

    <fragment
        android:id="@+id/navigation_filtro"
        android:name="com.lorenzohamaoka.proyectoclimb.ui.filtro.FiltroFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_buscar"
        android:name="com.lorenzohamaoka.proyectoclimb.ui.buscar.BuscarFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_buscar" />

    <fragment
        android:id="@+id/navigation_lista"
        android:name="com.lorenzohamaoka.proyectoclimb.ui.lista.ListaFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_map"
        android:name="com.lorenzohamaoka.proyectoclimb.ui.map.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" />
</navigation>
